<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rotateRollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="rotateFromDeg" format="float" />
        <attr name="rotateToDeg" format="float" />
        <attr name="rotatePivotX" format="fraction"/>
        <attr name="rotatePivotY" format="fraction" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="progressProgressIndeterminate" format="boolean" />
        <attr name="progressBarColor" format="color" />
        <attr name="progressRimColor" format="color" />
        <attr name="progressRimWidth" format="dimension" />
        <attr name="progressSpinSpeed" format="float" />
        <attr name="progressBarSpinCycleTime" format="integer" />
        <attr name="progressCircleRadius" format="dimension" />
        <attr name="progressFillRadius" format="boolean" />
        <attr name="progressBarWidth" format="dimension" />
        <attr name="progressLinearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProgressWheelNew">
        <attr name="pwText" format="string" />
        <attr name="pwTextColor" format="color" />
        <attr name="pwTextSize" format="dimension" />
        <attr name="pwBarColor" format="color" />
        <attr name="pwRimColor" format="color" />
        <attr name="pwRimWidth" format="dimension" />
        <attr name="pwSpinSpeed" format="dimension" />
        <attr name="pwDelayMillis" format="integer" />
        <attr name="pwCircleColor" format="color" />
        <attr name="pwRadius" format="dimension" />
        <attr name="pwBarWidth" format="dimension" />
        <attr name="pwBarLength" format="dimension" />
        <attr name="pwContourColor" format="color"/>
        <attr name="pwContourSize" format="dimension"/>
    </declare-styleable>
</resources>